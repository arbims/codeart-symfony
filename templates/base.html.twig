<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
            {{ vite_asset('js/main.jsx', []) }}
        {% endblock %}
    </head>
    <body>
        

        <header>
            {% include 'element/navbar.html.twig' %}
        </header>
        {% if app.user %}
     
            {% include 'element/sidebar.html.twig' %}
        {% endif %}
        <main class="main">
            {% include 'element/flash.html.twig' %}
            {% block body %}{% endblock %}
            <div id="app"></div>
        </main>
        {% include 'element/footer.html.twig' %}
    </body>
</html>
